        "better-auth": "^1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "drizzle-orm": "^0.40.0",
        "hono": "^4.7.4",
        "lucide-react": "^0.477.0",
        "motion": "^12.4.10",
        "next": "15.2.1",
        "next-themes": "^0.4.4",
        "npm-check-updates": "^17.1.15",
